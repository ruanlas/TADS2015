algoritmo "exercício 1"
// Função :
// Autor :
// Data : 25/03/2015
// Seção de Declarações 
var
numero: inteiro
inicio
      escreval ("Digite um Número:")
      leia (numero)
      se (numero >= 100) e (numero <= 200) entao
         escreval("Pertence ao intervalo de 100 a 200")
      senao
           escreval ("Não pertence ao intervalo de 100 a 200")
      fimse
fimalgoritmo
