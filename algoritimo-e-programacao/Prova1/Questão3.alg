algoritmo "Questão3"
// Função :
// Autor :
// Data : 15/04/2015
// Seção de Declarações 
var
   a, b: inteiro
inicio
// Seção de Comandos
  a <- 5
  b <- 10
  escreval (" Valor de A: ", a)
  escreval (" Valor de B: ", b)
  se (a=5) e (b=10) entao
     a <- 10
     b <- 5
  fimse

   escreval ("Novo valor de A: ", a)
   escreval ("Novo valor de B: ", b)

fimalgoritmo
