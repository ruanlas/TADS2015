algoritmo "semnome"
// Função :
// Autor :
// Data : 05/04/2015
// Seção de Declarações 
var
nota: inteiro
inicio
// Exercício teste comando "se"
   escreval("Digite a nota do aluno(a)")
   leia (nota)
   se nota >=5 entao
      escreval ("Aprovado")
   senao
        escreval ("Reprovado")
   fimse
fimalgoritmo
