algoritmo "semnome"
// Função :
// Autor :
// Data : 25/03/2015
// Seção de Declarações 
var
j: inteiro
y: inteiro
soma: inteiro
inicio
      escreval("Insira um número:")
      leia (y)
      j<- 1
      enquanto j <= y faca
           soma <- soma + j
           escreva (j," ")
      j<- j+1
      fimenquanto
      escreval
      escreval ("Resultado: ", soma)
fimalgoritmo
