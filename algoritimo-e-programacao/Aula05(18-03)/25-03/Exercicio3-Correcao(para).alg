algoritmo "semnome"
// Função :
// Autor :
// Data : 25/03/2015
// Seção de Declarações 
var
j: inteiro
y: inteiro
soma: inteiro
inicio
      escreval("Insira um número:")
      leia (y)
      para j de 1 ate y faca
           soma <- soma + j
           escreva (j," ")
      fimpara
      escreval
      escreval ("Resultado: ", soma)
fimalgoritmo
