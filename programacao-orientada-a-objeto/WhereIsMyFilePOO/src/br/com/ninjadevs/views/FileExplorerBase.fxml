<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="br.com.ninjadevs.controllers.FileExplorerBaseController">
   <top>
      <MenuBar BorderPane.alignment="CENTER">
        <menus>
            <Menu mnemonicParsing="false" text="Order By">
              <items>
                <MenuItem fx:id="MenuItemName" mnemonicParsing="false" onAction="#onClickMenuItemName" text="Name &lt;" />
                  <MenuItem fx:id="MenuItemNameA" mnemonicParsing="false" onAction="#onClickMenuItemNameA" text="Name &gt;" />
                  <MenuItem fx:id="MenuItemDate" mnemonicParsing="false" onAction="#onClickMenuItemDate" text="Date &lt;" />
                  <MenuItem fx:id="MenuItemDateA" mnemonicParsing="false" onAction="#onClickMenuItemDateA" text="Date &gt;" />
                  <MenuItem fx:id="MenuItemSize" mnemonicParsing="false" onAction="#onClickMenuItemSize" text="Size &lt;" />
                  <MenuItem fx:id="MenuItemSizeA" mnemonicParsing="false" onAction="#onClickMenuItemSizeA" text="Size &gt;" />
              </items>
            </Menu>
          <Menu mnemonicParsing="false" text="Help">
            <items>
              <MenuItem fx:id="MenuItemAbout" mnemonicParsing="false" onAction="#onClickMenuItemAbout" text="About" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </top>
</BorderPane>
